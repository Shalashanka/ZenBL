{
  "name": "zenox",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "apk:debug": "cd android && gradlew.bat :app:assembleDebug -x lint -x test --no-daemon -PreactNativeArchitectures=arm64-v8a",
    "apk:release": "cd android && gradlew.bat :app:assembleRelease -x lint -x test --no-daemon -PreactNativeArchitectures=arm64-v8a",
    "apk:install:debug": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-navigation/bottom-tabs": "^7.14.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.13.0",
    "expo": "~54.0.33",
    "expo-build-properties": "~1.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-haptics": "^15.0.8",
    "expo-linear-gradient": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "lucide-react-native": "^0.564.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.23.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.29.0",
    "@types/react": "~19.1.0",
    "babel-preset-expo": "^54.0.10",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react-native-worklets": "0.5.1"
  },
  "private": true,
  "codegenConfig": {
    "name": "NativeZenEngineSpec",
    "type": "modules",
    "jsSrcsDir": "specs",
    "android": {
      "javaPackageName": "com.zenox"
    }
  }
}
